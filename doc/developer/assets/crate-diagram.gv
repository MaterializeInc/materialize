digraph packages {
    subgraph cluster_demo {
        label = "/demo";
        color = blue;
        "billing-demo" [tooltip="Microservice demo using Materialize to power a real-time billing usecase",shape=Mdiamond,color=red];
    }
    subgraph cluster_fuzz {
        label = "/fuzz";
        color = blue;
        "fuzz" [tooltip="fuzz"];
