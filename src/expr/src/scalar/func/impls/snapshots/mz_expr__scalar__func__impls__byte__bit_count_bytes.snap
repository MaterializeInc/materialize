fn bit_count_bytes<'a>(a: &'a [u8]) -> Result<i64, EvalError> {
    let count: u64 = a.iter().map(|b| u64::cast_from(b.count_ones())).sum();
    i64::try_from(count)
        .or_else(|_| Err(EvalError::Int64OutOfRange(count.to_string().into())))
}
