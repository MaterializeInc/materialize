impl<'a> crate::func::EagerUnaryFunc<'a> for JsonbTypeof {
    type Input = JsonbRef<'a>;
    type Output = &'a str;
    fn call(&self, a: Self::Input) -> Self::Output {
        jsonb_typeof(a)
