#[allow(clippy::extra_unused_lifetimes)]
pub fn jsonb_typeof<'a>(a: JsonbRef<'a>) -> &'a str {
    {
        match a.into_datum() {
            Datum::Map(_) => "object",
            Datum::List(_) => "array",
            Datum::String(_) => "string",
            Datum::Numeric(_) => "number",
            Datum::True | Datum::False => "boolean",
            Datum::JsonNull => "null",
            d => panic!("Not jsonb: {:?}", d),
        }
    }
