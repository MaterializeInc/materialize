#[allow(clippy::extra_unused_lifetimes)]
pub fn cast_numeric_to_float32<'a>(a: Numeric) -> Result<f32, EvalError> {
    {
        let i = a.to_string().parse::<f32>().unwrap();
        if i.is_infinite() {
            Err(EvalError::Float32OutOfRange(i.to_string().into()))
        } else {
            Ok(i)
        }
    }
}
