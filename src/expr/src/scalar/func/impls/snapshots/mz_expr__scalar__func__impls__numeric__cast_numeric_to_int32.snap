#[allow(clippy::extra_unused_lifetimes)]
pub fn cast_numeric_to_int32<'a>(a: Numeric) -> Result<i32, EvalError> {
    {
        let mut a = a;
        {
            let mut cx = numeric::cx_datum();
            cx.round(&mut a);
            cx.clear_status();
            cx.try_into_i32(a).or(Err(EvalError::Int32OutOfRange(a.to_string().into())))
        }
    }
}
