#[allow(clippy::extra_unused_lifetimes)]
pub fn floor_numeric<'a>(a: Numeric) -> Numeric {
    {
        let mut a = a;
        {
            if a.exponent() >= 0 {
                return a;
            }
            let mut cx = numeric::cx_datum();
            cx.set_rounding(Rounding::Floor);
            cx.round(&mut a);
            numeric::munge_numeric(&mut a).unwrap();
            a
        }
    }
}
