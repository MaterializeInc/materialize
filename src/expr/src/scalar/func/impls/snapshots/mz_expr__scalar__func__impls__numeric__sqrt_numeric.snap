#[allow(clippy::extra_unused_lifetimes)]
pub fn sqrt_numeric<'a>(a: Numeric) -> Result<Numeric, EvalError> {
    {
        let mut a = a;
        {
            if a.is_negative() {
                return Err(EvalError::NegSqrt);
            }
            let mut cx = numeric::cx_datum();
            cx.sqrt(&mut a);
            numeric::munge_numeric(&mut a).unwrap();
            Ok(a)
        }
    }
}
