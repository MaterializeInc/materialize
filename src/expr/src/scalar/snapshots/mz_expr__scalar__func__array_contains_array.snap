fn array_contains_array<'a>(a: Array<'a>, b: Array<'a>) -> bool {
    let a = a.elements();
    let b = b.elements();
    if b.iter().contains(&Datum::Null) {
        false
    } else {
        b.iter().all(|item_b| a.iter().any(|item_a| item_a == item_b))
    }
}
