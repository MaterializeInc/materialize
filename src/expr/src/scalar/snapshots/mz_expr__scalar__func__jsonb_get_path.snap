pub struct JsonbGetPath;
impl<'a> crate::func::binary::EagerBinaryFunc<'a> for JsonbGetPath {
    type Input1 = JsonbRef<'a>;
    type Input2 = Array<'a>;
    type Output = Option<JsonbRef<'a>>;
    fn call(
        &self,
        a: Self::Input1,
        b: Self::Input2,
        temp_storage: &'a mz_repr::RowArena,
    ) -> Self::Output {
        jsonb_get_path(a, b)
    }
    fn output_type(
        &self,
        input_type_a: mz_repr::SqlColumnType,
        input_type_b: mz_repr::SqlColumnType,
    ) -> mz_repr::SqlColumnType {
