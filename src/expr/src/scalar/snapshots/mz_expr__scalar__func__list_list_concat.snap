fn list_list_concat<'a>(
    a: Option<DatumList<'a>>,
    b: Option<DatumList<'a>>,
    temp_storage: &'a RowArena,
) -> Option<DatumList<'a>> {
    match (a, b) {
        (None, b) => b,
        (a, None) => a,
        (Some(a), Some(b)) => {
            let a = a.iter();
            let b = b.iter();
            let datum = temp_storage.make_datum(|packer| packer.push_list(a.chain(b)));
            Some(datum.unwrap_list())
        }
    }
}
