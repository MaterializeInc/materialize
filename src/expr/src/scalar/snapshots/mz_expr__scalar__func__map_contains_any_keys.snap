fn map_contains_any_keys<'a>(map: DatumMap<'a>, keys: Datum<'a>) -> Option<bool> {
    if keys == Datum::Null {
        return None;
    }
    let keys = keys.unwrap_array();
    Some(
        keys
            .elements()
            .iter()
            .any(|key| !key.is_null() && map.iter().any(|(k, _v)| k == key.unwrap_str())),
    )
}
