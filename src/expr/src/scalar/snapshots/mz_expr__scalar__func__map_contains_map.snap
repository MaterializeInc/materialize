fn map_contains_map<'a>(a: Datum<'a>, b: Datum<'a>) -> Datum<'a> {
    let map_a = a.unwrap_map();
    b.unwrap_map()
        .iter()
        .all(|(b_key, b_val)| {
            map_a.iter().any(|(a_key, a_val)| (a_key == b_key) && (a_val == b_val))
        })
        .into()
}
