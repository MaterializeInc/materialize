fn map_contains_map<'a>(map_a: DatumMap<'a>, b: DatumMap<'a>) -> bool {
    b.iter()
        .all(|(b_key, b_val)| {
            map_a.iter().any(|(a_key, a_val)| (a_key == b_key) && (a_val == b_val))
        })
}
