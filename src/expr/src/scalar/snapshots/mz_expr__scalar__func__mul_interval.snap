fn mul_interval<'a>(a: Datum<'a>, b: Datum<'a>) -> Result<Datum<'a>, EvalError> {
    a.unwrap_interval()
        .checked_mul(b.unwrap_float64())
        .ok_or_else(|| EvalError::IntervalOutOfRange(format!("{a} * {b}").into()))
        .map(Datum::from)
}
