}
fn mz_render_typmod(oid: u32, typmod: i32) -> String {
    match Type::from_oid_and_typmod(oid, typmod) {
        Ok(typ) => typ.constraint().display_or("").to_string(),
        Err(_) if typmod >= 0 => format!("({typmod})"),
        Err(_) => "".into(),
    }
}
