fn sub_uint32<'a>(a: Datum<'a>, b: Datum<'a>) -> Result<Datum<'a>, EvalError> {
    a.unwrap_uint32()
        .checked_sub(b.unwrap_uint32())
        .ok_or(EvalError::UInt32OutOfRange(format!("{a} - {b}").into()))
        .map(Datum::from)
}
