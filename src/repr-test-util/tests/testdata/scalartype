build-scalar-type
int32
----
Int32

build-scalar-type
int64
----
Int64

build-scalar-type
bool
----
Bool

build-scalar-type
timestamp_tz
---

build-scalar-type
(decimal 10 2)
----
TimestampTz

build-scalar-type
(numeric 11)
----
Numeric { max_scale: Some(NumericMaxScale(11)) }

build-scalar-type
(array oid)
----
Array(Oid)

build-scalar-type
(list jsonb)
----
List { element_type: Jsonb, custom_id: None }

build-scalar-type
(list jsonb (user 100))
----
List { element_type: Jsonb, custom_id: Some(User(100)) }

build-scalar-type
(record
    [["col1name" (date true)]
     ["col2name" ((list (map bytes (user 98))) false)]
     ["col3name" ((list interval (user 34234)) false) ]]
     (user 0) "recordname")
----
Record { fields: [(ColumnName("col1name"), ColumnType { scalar_type: Date, nullable: true }), (ColumnName("col2name"), ColumnType { scalar_type: List { element_type: Map { value_type: Bytes, custom_id: Some(User(98)) }, custom_id: None }, nullable: false }), (ColumnName("col3name"), ColumnType { scalar_type: List { element_type: Interval, custom_id: Some(User(34234)) }, nullable: false })], custom_id: Some(User(0)), custom_name: Some("recordname") }

build-scalar-type
nonexistent
----
error: ScalarType::Nonexistent is not a supported enum.
