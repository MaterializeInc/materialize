# Bool: zero value
row-codec-golden
[false] [bool]
----
AAEAAAAAAAAAAQ==

# Bool: sanity check
row-codec-golden
[true] [bool]
----
AAEAAAAAAAAAAg==

# Int16: zero value
row-codec-golden
[0] [int16]
----
AAMAAAAAAAAAAwAA

# Int16: sanity check
row-codec-golden
[1] [int16]
----
AAMAAAAAAAAAAwEA

# Int16: min value
row-codec-golden
[-32768] [int16]
----
AAMAAAAAAAAAAwCA

# Int16: max value
row-codec-golden
[32767] [int16]
----
AAMAAAAAAAAAA/9/

