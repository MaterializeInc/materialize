build
select a from (select 1 as a, true as b) where (select b);
----
digraph G {
    compound = true
    labeljust = l
    label="select a from (select 1 as a, true as b) where (select b);
"
    node [ shape = box ]
    subgraph cluster5 {
        label = "Box5:Select"
        boxhead5 [ shape = record, label="{ Distinct: Preserve| 0: Q4.C0 }" ]
        {
            rank = same
            node [ shape = circle ]
            Q4 [ label="Q4(F)" ]
        }
    }
    subgraph cluster2 {
        label = "Box2:Select"
        boxhead2 [ shape = record, label="{ Distinct: Preserve| 0: Q1.C0| 1: Q1.C1| Q3.C0 }" ]
        {
            rank = same
            node [ shape = circle ]
            Q1 [ label="Q1(F)" ]
            Q3 [ label="Q3(S)" ]
            Q3 -> Q1 [ label = "correlation", style = filled, color = red ]
        }
    }
    subgraph cluster1 {
        label = "Box1:Select"
        boxhead1 [ shape = record, label="{ Distinct: Preserve| 0: 1| 1: true }" ]
        {
            rank = same
            node [ shape = circle ]
            Q0 [ label="Q0(F)" ]
        }
    }
    subgraph cluster0 {
        label = "Box0:Values"
        boxhead0 [ shape = record, label="{ Distinct: Preserve| ROW 0:  }" ]
        {
            rank = same
        }
    }
    subgraph cluster4 {
        label = "Box4:Select"
        boxhead4 [ shape = record, label="{ Distinct: Preserve| 0: Q1.C1 }" ]
        {
            rank = same
            node [ shape = circle ]
            Q2 [ label="Q2(F)" ]
        }
    }
    subgraph cluster3 {
        label = "Box3:Values"
        boxhead3 [ shape = record, label="{ Distinct: Preserve| ROW 0:  }" ]
        {
            rank = same
        }
    }
    edge [ arrowhead = none, style = dashed ]
    Q4 -> boxhead2 [ lhead = cluster2 ]
    Q1 -> boxhead1 [ lhead = cluster1 ]
    Q3 -> boxhead4 [ lhead = cluster4 ]
    Q0 -> boxhead0 [ lhead = cluster0 ]
    Q2 -> boxhead3 [ lhead = cluster3 ]
}

