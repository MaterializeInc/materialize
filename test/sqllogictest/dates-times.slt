# Test ISO-formatted timestamps

query T
SELECT TIMESTAMP '2007-02-01T15:04:05'
----
2007-02-01 15:04:05

query T
SELECT TIMESTAMPTZ '2007-02-01T15:04:05+00'
----
2007-02-01 15:04:05+00

query T
SELECT TIMESTAMPTZ '20070201 T 15:04:05+00';
