# Test string to datetime types

query T
SELECT '2007-02-01'::date
----
2007-02-01

query T
SELECT '-1-2 3 -4:5:6.7'::interval;
----
-1 year -2 months +2 days +19:54:53.3

query T
SELECT '01:23:45'::time;
----
01:23:45

query T
SELECT '2007-02-01 15:04:05'::timestamp;
----
