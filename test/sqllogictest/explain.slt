mode cockroach

query T multiline
EXPLAIN RAW PLAN FOR SELECT * FROM (SELECT 1)
----
%0 =
| Constant ()
| Map 1

EOF

query T multiline
EXPLAIN RAW PLAN FOR SELECT (SELECT (SELECT 1))
----
%0 =
| Constant ()
| Map select(%2)
| |
| | %2 =
| | | Constant ()
| | | Map select(%4)
| | | |
| | | | %4 =
| | | | | Constant ()
| | | | | Map 1
| | | |
| |

EOF

query T multiline
EXPLAIN TYPED RAW PLAN FOR SELECT * FROM (SELECT 1)
----
%0 =
| Constant ()
| | types = ()
| | keys = ()
| Map 1
| | types = (i32)
| | keys = ()

EOF

query T multiline
EXPLAIN DECORRELATED PLAN FOR SELECT * FROM (SELECT 1)
----
%0 =
| Constant ()

%1 =
| Get %0

%2 =
| Constant ()

%3 =
| Join %1 %2
| | implementation = Unimplemented
| Map 1

EOF

query T multiline
EXPLAIN OPTIMIZED PLAN FOR SELECT * FROM (SELECT 1)
----
%0 =
| Constant (1)

EOF

query T multiline
EXPLAIN PLAN FOR SELECT * FROM (SELECT 1)
----
%0 =
| Constant (1)

EOF

