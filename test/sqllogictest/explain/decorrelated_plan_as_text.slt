# Test EXISTS subqueries.
query T multiline
EXPLAIN DECORRELATED PLAN AS TEXT FOR
SELECT * FROM t WHERE EXISTS(SELECT * FROM mv WHERE t.a < mv.a) AND EXISTS(SELECT * FROM mv WHERE t.b > mv.b)
----
Return
  Project (#0, #1)
    Filter #2
      Project (#0, #1, #3)
        Join on=(#1 = #2)
          Get l3
          Union
            Get l5
            CrossJoin
              Project (#0)
                Join on=(#0 = #1)
                  Union
                    Negate
                      Distinct group_by=[#0]
                        Get l5
                    Distinct group_by=[#0]
                      Get l4
                  Get l4
              Constant
                - (false)
With
  cte l5 =
    Map (true)
      Distinct group_by=[#0]
        Filter (#0 > #2)
          CrossJoin
            Get l4
            Get materialize.public.mv
  cte l4 =
    Distinct group_by=[#1]
      Get l3
  cte l3 =
    Project (#0, #1)
      Filter #2
        Project (#0, #1, #3)
          Join on=(#0 = #2)
            Get l0
            Union
              Get l2
              CrossJoin
                Project (#0)
                  Join on=(#0 = #1)
                    Union
                      Negate
                        Distinct group_by=[#0]
                          Get l2
                      Distinct group_by=[#0]
                        Get l1
                    Get l1
                Constant
                  - (false)
  cte l2 =
    Map (true)
      Distinct group_by=[#0]
        Filter (#0 < #1)
          CrossJoin
            Get l1
            Get materialize.public.mv
  cte l1 =
    Distinct group_by=[#0]
      Get l0
  cte l0 =
    Filter (true AND true)
      CrossJoin
        Constant
          - ()
        Get materialize.public.t

EOF

