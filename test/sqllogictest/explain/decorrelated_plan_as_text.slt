# Test Reduce (global).
query T multiline
EXPLAIN DECORRELATED PLAN AS TEXT FOR
SELECT abs(min(a) - max(a)) FROM t
----
Return
  Project (#2)
    Project (#0..=#2)
      Map (abs((#0 - #1)))
        Get l2
With
  cte l2 =
    Union
      Get l1
      CrossJoin
        Project ()
          CrossJoin
            Union
              Negate
                Distinct
                  Get l1
              Distinct
                Get l0
            Get l0
        Constant
          - (null, null)
  cte l1 =
    Reduce aggregates=[min(#0), max(#0)]
      CrossJoin
        Get l0
        Get materialize.public.t
  cte l0 =
    Constant
      - ()

EOF

