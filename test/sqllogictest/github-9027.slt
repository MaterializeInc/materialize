query I
SELECT
 1
FROM
 lineitem JOIN orders ON true
WHERE
 l_orderkey
 = ANY (
 SELECT
 o_orderkey
 FROM
 lineitem RIGHT JOIN orders ON true
 WHERE
 l_receiptdate IN ('1997-07-06')
 OR l_commitdate = o_orderdate - ' 7 DAY '
 );
