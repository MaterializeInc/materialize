# Test that cluster ids aren't reused.

mode cockroach

# Requires stable ids
reset-server

statement ok
CREATE SCHEMA foo

query IT rowsort
SELECT id, name FROM mz_schemas
----
1 mz_catalog
2 pg_catalog
3 public
4 mz_internal
5 information_schema
6 foo

statement ok
DROP schema foo

statement ok
CREATE schema bar

query IT rowsort
SELECT id, name FROM mz_schemas
----
1 mz_catalog
2 pg_catalog
3 public
4 mz_internal
5 information_schema
7 bar

statement ok
CREATE DATABASE foo

query IT rowsort
SELECT id, name FROM mz_databases
----
1 materialize
2 foo

statement ok
DROP DATABASE foo

statement ok
CREATE DATABASE bar

query IT rowsort
SELECT id, name FROM mz_databases
----
1 materialize
3 bar

statement ok
CREATE ROLE foo

query TT rowsort
SELECT id, name FROM mz_roles
----
s1 mz_system
s2 mz_introspection
u1 materialize
u2 foo

statement ok
DROP ROLE foo

statement ok
CREATE ROLE bar

query TT rowsort
SELECT id, name FROM mz_roles
----
s1 mz_system
s2 mz_introspection
u1 materialize
u3 bar

statement ok
CREATE CLUSTER foo REPLICAS (r1 (size '1'))

query TT rowsort
SELECT id, name FROM mz_clusters
----
s1 mz_system
s2 mz_introspection
u1 default
u2 foo

statement ok
DROP CLUSTER foo CASCADE

statement ok
CREATE CLUSTER bar REPLICAS (r1 (size '1'))

query TT rowsort
SELECT id, name FROM mz_clusters
----
s1 mz_system
s2 mz_introspection
u1 default
u3 bar
