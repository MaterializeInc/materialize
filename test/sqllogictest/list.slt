mode cockroach

query T
select LIST[1,2,3]
----
{1,2,3}

query error Cannot create list with mixed types
select LIST[1,LIST[2,3]]

query T
select LIST[LIST[1],LIST[2,3]]
----
{{1},{2,3}}

query error Cannot assign type to this empty list
select LIST[]

query T
select LIST[] :: INT LIST
----
{}

query T
select LIST[null]
----
{NULL}

query T
select LIST[1, null]
----
{1,NULL}

query T
select LIST[1, null] :: INT LIST
----
{1,NULL}

query error
select LIST[1, null] :: TEXT LIST

query T
select LIST['foo', 'f}o', '"\', null, 'null', 'NULL']
----
{foo,"f}o","\"\\",NULL,null,"NULL"}

query T
select list[list[list['"']]];
----
{{{"\""}}}

query T
select list['{1}']
----
{"{1}"}

query T
SELECT LIST[LIST[]::text list, LIST['a', 'b'], LIST['z']]
----
{{},{a,b},{z}}
