query T
SELECT hmac('Hi There', '\x0b0b0b0b0b0b0b0b0b0b0b0b0b0b0b0b'::bytea, 'md5')::text
----
\x9294727a3638bb1c13f48ef8158bfc9d

query T
SELECT hmac('Jefe', 'what do ya want for nothing?', 'md5')::text
----
\x813aead7c4a34bff01a16d61368e7c13

