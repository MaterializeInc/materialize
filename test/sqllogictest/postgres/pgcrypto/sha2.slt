# SHA224

query T
SELECT digest('', 'sha224')::text
----
\xd14a028c2a3a2bc9476102bb288234c415a2b01f828ea62ac5b3e42f

query T
SELECT digest('a', 'sha224')::text
----
\xabd37534c7d9a2efb9465de931cd7055ffdb8879563ae98078d6d6d5

query T
SELECT digest('abc', 'sha224')::text
----
\x23097d223405d8228642a477bda255b32aadbce4bda0b3f7e36c9da7

query T
SELECT digest('abcdbcdecdefdefgefghfghighijhijkijkljklmklmnlmnomnopnopq', 'sha224')::text
----
\x75388b16512776cc5dba5da1fd890150b0c6455cb4f58b1952522525

query T
SELECT digest('12345678901234567890123456789012345678901234567890123456789012345678901234567890', 'sha224')::text
----
\xb50aecbe4e9bb0b57bc5f3ae760a8e01db24f203fb3cdcd13148046e

