mode cockroach

statement ok
CREATE TABLE t (
    v integer
)

statement ok
INSERT INTO t VALUES
    (1),
    (2)

statement ok
INSERT INTO "t" VALUES
    (3)

query I
SELECT * FROM "t" ORDER BY v
----
1
2
3

query I
SELECT * FROM t ORDER BY "v"
----
1
2
3

query T
SELECT count(*) AS "count" FROM t
----
3

query T
SELECT count(*) AS count FROM "t"
----
3

query T
SELECT count(*) AS "count" FROM "t"
----
3

