# Check that we correctly invert the filter here via De Morgan.
query T multiline
EXPLAIN PLAN FOR SELECT b > ALL(SELECT a FROM x) FROM y
----
%0 = Let l0 =
| Get materialize.public.y (u27)
| Distinct group=(#0)

%1 =
| Get %0 (l0)
| ArrangeBy ()

%2 =
| Get materialize.public.x (u25)

%3 = Let l1 =
| Join %1 %2
| | implementation = Differential %2 %1.()
| | demand = (#0, #1)
| Filter (#0 <= #1)
| Distinct group=(#0)

%4 =
| Get materialize.public.y (u27)
| ArrangeBy (#0)

%5 =
| Get %3 (l1)
| Map true

%6 =
| Get %3 (l1)
| Negate

%7 =
| Union %6 %0
| Map false

%8 =
| Union %5 %7

%9 =
| Join %4 %8 (= #0 #1)
| | implementation = Differential %8 %4.(#0)
| | demand = (#2)
| Map !(#2)
| Project (#3)

EOF

