mode cockroach

query TT
SELECT * FROM mz_system_configuration;
----

statement ok
ALTER SYSTEM SET wal_level TO logical

query TT
SELECT * FROM mz_system_configuration;
----
wal_level   {"String":"logical"}

statement ok
ALTER SYSTEM SET wal_level = 'physical'

query TT
SELECT * FROM mz_system_configuration;
----
wal_level   {"String":"physical"}

statement ok
ALTER SYSTEM SET shared_buffers = 5

query TT
SELECT * FROM mz_system_configuration;
----
shared_buffers  {"Number":"5"}
wal_level       {"String":"physical"}
