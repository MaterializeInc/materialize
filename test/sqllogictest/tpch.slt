query T multiline
-- Query 07
EXPLAIN PLAN FOR SELECT
    supp_nation,
    cust_nation,
    l_year,
    sum(volume) AS revenue
FROM
    (
        SELECT
            n1.n_name AS supp_nation,
            n2.n_name AS cust_nation,
            extract(year FROM l_shipdate) AS l_year,
            l_extendedprice * (1 - l_discount) AS volume
        FROM
            supplier,
            lineitem,
            orders,
            customer,
            nation n1,
            nation n2
        WHERE
            s_suppkey = l_suppkey
            AND o_orderkey = l_orderkey
            AND c_custkey = o_custkey
            AND s_nationkey = n1.n_nationkey
            AND c_nationkey = n2.n_nationkey
            AND (
                (n1.n_name = 'FRANCE' AND n2.n_name = 'GERMANY')
                or (n1.n_name = 'GERMANY' AND n2.n_name = 'FRANCE')
            )
            AND l_shipdate BETWEEN DATE '1995-01-01' AND DATE '1996-12-31'
    ) AS shipping
GROUP BY
    supp_nation,
    cust_nation,
    l_year
ORDER BY
    supp_nation,
    cust_nation,
    l_year
----
%0 = Let l0 =
| Get materialize.public.nation (u1)
| ArrangeBy (#0)

%1 =
| Get materialize.public.supplier (u8)
| ArrangeBy (#0) (#3)

%2 =
| Get materialize.public.lineitem (u21)
| ArrangeBy (#0) (#2)

%3 =
| Get materialize.public.orders (u18)
| ArrangeBy (#0) (#1)

%4 =
| Get materialize.public.customer (u15)
| ArrangeBy (#0) (#3)

%5 =
| Join %1 %2 %3 %4 %0 %0 (= #0 #9) (= #3 #40) (= #7 #23) (= #24 #32) (= #35 #44)
| | implementation = DeltaQuery
| |   delta %1 %0.(#0) %2.(#2) %3.(#0) %4.(#0) %0.(#0)
| |   delta %2 %1.(#0) %3.(#0) %4.(#0) %0.(#0) %0.(#0)
| |   delta %3 %4.(#0) %0.(#0) %2.(#0) %1.(#0) %0.(#0)
| |   delta %4 %0.(#0) %3.(#1) %2.(#0) %1.(#0) %0.(#0)
| |   delta %0 %1.(#3) %2.(#2) %3.(#0) %4.(#0) %0.(#0)
| |   delta %0 %4.(#3) %3.(#1) %2.(#0) %1.(#0) %0.(#0)
| Filter (((#41 = "FRANCE") && (#45 = "GERMANY")) || ((#41 = "GERMANY") && (#45 = "FRANCE"))), (#17 <= 1996-12-31), (#17 >= 1995-01-01)
| Project (#12, #13, #17, #41, #45)
| Reduce group=(#3, #4, date_part_year_tstz(datetotstz(#2)))
| | agg sum((#0 * (1 - #1)))

Finish order_by=(#0 asc, #1 asc, #2 asc) limit=none offset=0 project=(#0..#3)

EOF


