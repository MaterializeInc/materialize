        Join on=(#0 = #1) type=differential // { arity: 2 }
          implementation
            %0:t1[#0]K » %1:t2[#0]K
          ArrangeBy keys=[[#0]] // { arity: 1 }
            Project (#0) // { arity: 1 }
              Filter (#0) IS NOT NULL // { arity: 2 }
                Get materialize.public.t1 // { arity: 2 }
          ArrangeBy keys=[[#0]] // { arity: 1 }
            Project (#0) // { arity: 1 }
