# Inline binary expressions.

query T multiline
EXPLAIN PLAN FOR
  SELECT a FROM (SELECT a, a = 3 AS cond, u != 2 as cond2 FROM x) WHERE NOT(cond) AND NOT(cond2)
----
Source materialize.public.x (u1):
| Filter (#1 = 2), (#0 != 3)
| Project (#0, #1)

Query:
%0 =
| Get materialize.public.x (u1)
| Filter (#1 = 2), (#0 != 3)
| Project (#0)

EOF

# Don't inline binary expressions not having a leaf argument.

query T multiline
EXPLAIN PLAN FOR
  SELECT a FROM (SELECT a, (a + 1) = (u + 3) AS cond FROM x) WHERE NOT(cond)
----
Source materialize.public.x (u1):
| Project (#0, #1)

Query:
%0 =
| Get materialize.public.x (u1)
| Filter ((#0 + 1) != (#1 + 3))
| Project (#0)

EOF

# We can push down b = 3, since it only refers to input columns. We can't push
# down c = 3 since the computation of c refers to mapped columns.

query T multiline
EXPLAIN PLAN FOR
  SELECT * FROM
    (SELECT a, b, b+1 as c FROM
      (SELECT a, a+1 as b FROM y))
WHERE b = 3 AND c = 3
----
Source materialize.public.y (u2):
| Filter (3 = (#0 + 1))
| Project (#0)

Query:
%0 =
| Get materialize.public.y (u2)
| Filter (3 = (#0 + 1))
| Map (#0 + 1), (#1 + 1)
| Filter (#2 = 3)

EOF

# Regression test to ensure that a filter gets pushed down to all inputs of a union.

query T multiline
EXPLAIN DECORRELATED PLAN FOR
SELECT a FROM (SELECT DISTINCT a FROM x UNION ALL SELECT a FROM y) WHERE a = 3
----
%0 = Let l0 =
| Constant ()

%1 =
| Get materialize.public.x (u1)

%2 = Let l1 =
| Join %0 %1
| | implementation = Unimplemented
| Project (#0)
| Distinct group=(#0)

%3 =
| Get %2 (l1)
| Map #0
| Project (#0, #1)
| Project (#1)

%4 =
| Get materialize.public.y (u2)

%5 = Let l2 =
| Join %0 %4
| | implementation = Unimplemented

%6 =
| Get %5 (l2)
| Map #0
| Project (#0, #1)
| Project (#1)

%7 =
| Union %3 %6
| Filter (#0 = 3)

EOF

query T multiline
EXPLAIN PLAN FOR
SELECT a FROM (SELECT DISTINCT a FROM x UNION ALL SELECT a FROM y) WHERE a = 3
----
Source materialize.public.x (u1):
| Filter (#0 = 3)
| Project (#0)

Source materialize.public.y (u2):
| Filter (#0 = 3)
| Project (#0)

Query:
%0 =
| Get materialize.public.x (u1)
| Filter (#0 = 3)
| Project ()
| Distinct group=()
| Map 3

%1 =
| Get materialize.public.y (u2)
| Filter (#0 = 3)

%2 =
| Union %0 %1

EOF

