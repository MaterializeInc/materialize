#
# Tests with a relation that has columns that do not participate in the key
#

statement ok
CREATE TABLE t2 (f1 INTEGER, f2 INTEGER, f3 INTEGER, f4 INTEGER, PRIMARY KEY (f1, f2));

query T multiline
EXPLAIN WITH(arity, join implementations) SELECT DISTINCT f1, f3 FROM t2 WHERE f2 = f3;
----
Explained Query:
  Project (#0, #2) // { arity: 2 }
    Filter (#1 = #2) // { arity: 4 }
      ReadStorage materialize.public.t2 // { arity: 4 }

Source materialize.public.t2
  filter=((#1 = #2))

EOF

query T multiline
EXPLAIN WITH(arity, join implementations) SELECT DISTINCT f3 FROM t2 WHERE f1 = f2 AND f2 = f3;
----
Explained Query:
  Project (#2) // { arity: 1 }
    Filter (#0 = #1) AND (#1 = #2) // { arity: 4 }
      ReadStorage materialize.public.t2 // { arity: 4 }

Source materialize.public.t2
  filter=((#0 = #1) AND (#1 = #2))

EOF

