#
# CREATE a view containing various time literals
#

> CREATE MATERIALIZED VIEW time_literals AS SELECT DATE '2007-02-01', INTERVAL '1-2 3 4:5:6.7', TIME '01:23:45', TIMESTAMP '2007-02-01 15:04:05.123456', TIMESTAMPTZ '2007-02-01 15:04:05.123456+06';
